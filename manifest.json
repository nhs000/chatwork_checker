{
    "manifest_version": 2,

    "name": "Chatwork Check",
    "description": "Add confirm dialog when submitting form in chatwork. ",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": ["check.js"]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
